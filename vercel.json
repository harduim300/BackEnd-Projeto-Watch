{
  "version": 2,
  "builds": [
    {
      "src": "./dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "./dist/server.js"
    },
    {
      "src": "/tasks/(.*)",
      "dest": "./dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "./dist/server.js"
    }
  ]
}
  